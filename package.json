{
  "name": "jscompile",
  "version": "0.1.0",
  "description": "Compile, optimize and compress anything to JS.",
  "homepage": "https://github.com/cjpatoilo/jscompile",
  "repository": "cjpatoilo/jscompile",
  "license": "MIT",
  "author": "CJ Patoilo <cjpatoilo@gmail.com>",
  "bin": "cli.js",
  "main": "index.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "keywords": [
    "automation",
    "autoprefixer",
    "cli",
    "coffee",
    "coffeescript",
    "compile",
    "compress",
    "ecma",
    "ecmascript",
    "ecmascripts",
    "elm",
    "es2015",
    "es2016",
    "es5",
    "es6",
    "es7",
    "framework",
    "javascript",
    "js",
    "minified",
    "npm",
    "optimize",
    "package",
    "pure",
    "purs",
    "script",
    "scripts",
    "typescript"
  ],
  "dependencies": {
    "babel-preset-latest": "^6.16.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.2"
  },
  "devDependencies": {
    "ava": "^3.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier-standard": "^16.4.1"
  },
  "scripts": {
    "lint": "prettier-standard --check",
    "test": "ava"
  },
  "engines": {
    "node": "^12.18.0",
    "npm": "^6.14.5"
  },
  "prettier": {
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier-standard --format",
      "git add"
    ]
  }
}
