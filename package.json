{
	"name": "jscompile",
	"version": "0.0.0",
	"description": "Compile, optimize and compress anything to JS.",
	"homepage": "https://github.com/cjpatoilo/jscompile",
	"repository": "cjpatoilo/jscompile",
	"license": "MIT",
	"author": "CJ Patoilo <cjpatoilo@gmail.com>",
	"bin": "bin/cli.js",
	"main": "src/index.js",
	"files": [
		"bin/cli.js",
		"src/index.js"
	],
	"keywords": [
		"automation",
		"autoprefixer",
		"cli",
		"compile",
		"compress",
		"ecma",
		"ecmascript",
		"ecmascripts",
		"elm",
		"es",
		"es5",
		"es6",
		"es7",
		"es8",
		"framework",
		"javascript",
		"js",
		"minified",
		"npm",
		"optimize",
		"package",
		"pure",
		"script",
		"scripts",
		"typescript"
	],
	"ignore": [
		".appveyor.yml",
		".editorconfig",
		".github",
		".gitignore",
		".travis.yml"
	],
	"dependencies": {
		"babel-preset-latest": "^6.16.0",
		"gulp": "^3.9.1",
		"gulp-babel": "^6.1.2",
		"gulp-uglify": "^2.0.0"
	},
	"devDependencies": {
		"ava": "^0.17.0",
		"editorconfig-tools": "^0.1.1",
		"eslint": "^3.12.1",
		"eslint-config-styled": "^0.0.0",
		"husky": "^0.12.0",
		"nyc": "^10.0.0"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"precommit": "npm test",
		"test": "nyc ava && eslint . -c styled --ignore-path .gitignore && editorconfig-tools check ."
	}
}
